{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node-terminal",
            "name": "Run dev",
            "request": "launch",
            "command": "npm run dev",
            "outFiles": ["${workspaceFolder}/build/**/*.js"]
        },
        {
            "type": "node-terminal",
            "name": "Run dev:dev",
            "request": "launch",
            "command": "npm run dev:dev",
            "outFiles": ["${workspaceFolder}/build/**/*.js"]
        },
        {
            "type": "node-terminal",
            "name": "Run watch",
            "request": "launch",
            "command": "npm run watch",
            "outFiles": ["${workspaceFolder}/build/**/*.js"]
        },
        {
            "type": "node-terminal",
            "name": "Run watch:dev",
            "request": "launch",            
            "command": "npm run watch:dev",
            "outFiles": ["${workspaceFolder}/build/**/*.js"]


        },
        {
            "type": "node-terminal",
            "name": "Run start",
            "request": "launch",
            "command": "npm start"
        },
        {
            "type": "node-terminal",
            "name": "Run start:dev",
            "request": "launch",
            "command": "npm run start:dev"
        },
    ]
}